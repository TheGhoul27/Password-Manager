{
  "name": "password-manager",
  "version": "3.0.0",
  "private": true,
  "author": "Pradhumna Guruprasad",
  "description": "A password manager software.",
  "main": "public/electron.js",
  "homepage": "./",
  "license": "MIT",
  "build": {
    "appId": "react.electron.python.password-manager",
    "productName": "Password Manager",
    "copyright": "Copyright Â© 2022 Pradhumna Guruprasad",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "extraResources": [
      "scripts"
    ],
    "win": {
      "publisherName": "https://theghoul27.github.io/PassManage/",
      "legalTrademarks": "(c) 2022 https://theghoul27.github.io/PassManage/",
      "icon": "build/favicon.png",
      "target": ["nsis", "zip"],
      "asar": true
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "bootstrap": "^4.6.0",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "electron-is-dev": "^1.1.0",
    "events": "^3.3.0",
    "pidtree": "^0.5.0",
    "react": "^17.0.2",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^17.0.2",
    "react-fontawesome": "^1.7.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "tree-kill": "^1.2.2",
    "web-vitals": "^1.1.1",
    "xmlrpc": "^1.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "concurrently \"cross-env BROWSER=none npm run start\" \"wait-on http://localhost:3000 && electron .\" -k",
    "build-electron": "electron-builder",
    "electron-build": "npm run build && npm run build-electron -wml",
    "postinstall": "electron-builder install-app-deps",
    "strt": "electron ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "electron": "^7.1.0",
    "electron-builder": "^22.1.0",
    "wait-on": "^3.3.0"
  }
}
